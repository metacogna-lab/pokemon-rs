{
  "name": "pokemon-ts-agents",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript agents: strategic planner, RL model runner, game interaction orchestrator",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9",
    "@types/bun": "latest",
    "eslint": "^9",
    "typescript": "^5",
"typescript-eslint": "^8",
    "yaml": "^2"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "bun test",
    "test:e2e": "RUN_E2E=1 bun test e2e",
    "generate:client": "cd .. && bunx @openapitools/openapi-generator-cli generate -i openapi.yaml -g typescript-fetch -o ./agents/ts-client --additional-properties=supportsES6=true"
  }
}
